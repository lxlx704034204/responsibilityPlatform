<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="orbit-taskdist" namespace="/taskdist" extends="orbit-default">
    	<action name="indexlist" class="taskDistActions" method="pageIndexList">
			<result>/views/taskdist/taskdistlist.jsp</result>
		</action>
		<action name="indexgraph" class="taskDistActions" method="pageIndexGraph">
			<result>/views/taskdist/taskdistgraph.jsp</result>
		</action>
    </package>
    
    <package name="orbit-json-taskdist" namespace="/json/taskdist" extends="json-default">
        <action name="getModelTasks" class="taskDistActions" method="jsonGetModelTasks">
            <result type="json"></result>
        </action>
    </package>
    
</struts>