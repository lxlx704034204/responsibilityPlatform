<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="orbit-regular-levelthree" namespace="/regular/levelthree" extends="orbit-default">
        <action name="index" class="com.orbit.actions.LevelThreeLimitActions" method="pageIndex">
            <result>/views/regular/level3rdlimit.jsp</result>
        </action>
    </package>

    <package name="orbit-json-regular-levelthree" namespace="/json/regular/levelthree" extends="json-default">
        <action name="getListByPager" class="com.orbit.actions.LevelThreeLimitActions" method="jsonGetListByPager">
            <result type="json"></result>
        </action>
        <action name="batchUpdate" class="com.orbit.actions.LevelThreeLimitActions" method="jsonBatchUpdate">
            <result type="json"></result>
        </action>
    </package>
    
</struts>
